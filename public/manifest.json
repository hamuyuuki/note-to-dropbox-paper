{
  "name": "Note to Dropbox Paper",
  "version": "0.0.1",
  "description": "Note a web page to Dropbox Paper in Google Chrome",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js", "js/vendor.js"],
    "persistent": false
  },
  "permissions": ["storage", "identity", "https://www.dropbox.com/*", "tabs"]
}
