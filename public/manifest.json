{
  "manifest_version": 2,
  "name": "Note to Dropbox Paper",
  "description": "Note a web page to Dropbox Paper in Google Chrome",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js",
      "js/vendor.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "identity",
    "https://www.dropbox.com/*",
    "tabs"
  ]
}