{
  "manifest_version": 2,
  "name": "Note to Dropbox Paper",
  "description": "Note a web page to Dropbox Paper",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "storage",
    "<all_urls>"
  ]
}